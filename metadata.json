{
  "name": "opuscodium-firefox",
  "version": "3.0.0",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages the Firefox browser",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-firefox",
  "project_page": "https://github.com/opus-codium/puppet-firefox",
  "issues_url": "https://github.com/opus-codium/puppet-firefox/issues",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.0.0 < 7"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 8.0.0"
    }
  ]
}
